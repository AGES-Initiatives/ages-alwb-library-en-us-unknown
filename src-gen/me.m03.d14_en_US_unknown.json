{'id_' : 'me|m03|d14_en_US_unknown|meMA|C1Acrostic|text', 'text' : ''}
{'id_' : 'me|m03|d14_en_US_unknown|meMA|Ode1C1H|ode', 'text' : 'Ode i.'}
{'id_' : 'me|m03|d14_en_US_unknown|meMA|Ode1C1H|mode', 'text' : 'Mode 2.'}
