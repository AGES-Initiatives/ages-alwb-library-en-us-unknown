meMA.Ode1C21.ode = miscellanea_en_US_unknown.misc.Ode1
meMA.Ode1C21.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode1C21.greekmelody = he.h.m4_en_US_unknown.heHE.ThalassisToErythraion.greekname
meMA.Ode1C21.melody = he.h.m4_en_US_unknown.heHE.ThalassisToErythraion.notmetered
meMA.Ode1C21.incipit = "O Christ, I am eager…"
meMA.Ode1C21.text = "O Christ, I am eager to extol the host of Your Apostles. By their entreaties, O God, grant me a ray of the Holy Spirit and the lamp of Your wisdom."

meMA.Ode1C22.ode = miscellanea_en_US_unknown.misc.Ode1
meMA.Ode1C22.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode1C22.melody = he.h.m4_en_US_unknown.heHE.ThalassisToErythraion.notmetered
meMA.Ode1C22.incipit = "O Christ, Your holy Apostles…"
meMA.Ode1C22.text = "O Christ, Your holy Apostles were strengthened by Your strength and grace, and they crushed the might of the inimical foes. By their unceasing longing for You they became luminous."

meMA.Ode1C23.ode = miscellanea_en_US_unknown.misc.Ode1
meMA.Ode1C23.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode1C23.melody = he.h.m4_en_US_unknown.heHE.ThalassisToErythraion.notmetered
meMA.Ode1C23.incipit = "O Master, the illustrious Apostles…"
meMA.Ode1C23.text = "O Master, the illustrious Apostles did cures and healings in Your name, they captivated the Gentiles with the knowledge of You, and made them shine with Your divine light."

meMA.Ode1C24.ode = miscellanea_en_US_unknown.misc.Ode1
meMA.Ode1C24.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode1C24.melody = he.h.m4_en_US_unknown.heHE.ThalassisToErythraion.notmetered
meMA.Ode1C24.incipit = "Glorious Apostles of Christ, as…"
meMA.Ode1C24.text = "Glorious Apostles of Christ, as you were instructed in heavenly wisdom, with the zeal of your preaching, you showed all the vain sophistry of the wise to be foolishness."

meMA.Ode1C2T.ode = miscellanea_en_US_unknown.misc.Ode1
meMA.Ode1C2T.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode1C2T.melody = he.h.m4_en_US_unknown.heHE.ThalassisToErythraion.notmetered
meMA.Ode1C2T.incipit = "O all-immaculate Virgin Mother, you…"
meMA.Ode1C2T.text = "O all-immaculate Virgin Mother, you gave birth to One of the persons of the Holy Trinity. He came forth from you in flesh like ours, by the good pleasure of His Father and with the co-operation of the Holy Spirit."

meMA.Ode3C1H.ode = miscellanea_en_US_unknown.misc.Ode3
meMA.Ode3C1H.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode3C1H.greekmelody = he.h.m4_en_US_unknown.heHE.TousSousYmnologous.greekname
meMA.Ode3C1H.melody = he.h.m4_en_US_unknown.heHE.TousSousYmnologous.melody
meMA.Ode3C1H.incipit = he.h.m4_en_US_unknown.heHE.TousSousYmnologous.incipit
meMA.Ode3C1H.text = he.h.m4_en_US_unknown.heHE.TousSousYmnologous.text

meMA.Ode3C21.ode = miscellanea_en_US_unknown.misc.Ode3
meMA.Ode3C21.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode3C21.greekmelody = he.h.m4_en_US_unknown.heHE.EffrainetaiEpiSoi.greekname
meMA.Ode3C21.melody = he.h.m4_en_US_unknown.heHE.EffrainetaiEpiSoi.notmetered
meMA.Ode3C21.incipit = "O heralds of Christ, you…"
meMA.Ode3C21.text = "O heralds of Christ, you spoke with tongues of fire, and with divine power you declared and shared heavenly and divine doctrines on earth."

meMA.Ode3C22.ode = miscellanea_en_US_unknown.misc.Ode3
meMA.Ode3C22.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode3C22.melody = he.h.m4_en_US_unknown.heHE.EffrainetaiEpiSoi.notmetered
meMA.Ode3C22.incipit = "O Lord, You made Your…"
meMA.Ode3C22.text = "O Lord, You made Your disciples to be like the heavens, for to the ends of the earth they declared Your glory."

meMA.Ode3C23.ode = miscellanea_en_US_unknown.misc.Ode3
meMA.Ode3C23.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode3C23.melody = he.h.m4_en_US_unknown.heHE.EffrainetaiEpiSoi.notmetered
meMA.Ode3C23.incipit = "You are enrolled in heaven…"
meMA.Ode3C23.text = "You are enrolled in heaven, O all-wise Apostles, and you live with Christ. We who honor you pray that you protect us."

meMA.Ode3C2T.ode = miscellanea_en_US_unknown.misc.Ode3
meMA.Ode3C2T.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode3C2T.melody = he.h.m4_en_US_unknown.heHE.EffrainetaiEpiSoi.notmetered
meMA.Ode3C2T.incipit = "O all-pure Virgin, He who…"
meMA.Ode3C2T.text = "O all-pure Virgin, He who dwells on high has lived among us. He appeared to us, when without seed He became incarnate of you."

meMA.Ode3Kathisma1.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode3Kathisma1.greekmelody = he.a.m4_en_US_unknown.heAU.OYpsotheisEnToStavro.greekname
meMA.Ode3Kathisma1.melody = he.a.m4_en_US_unknown.heAU.OYpsotheisEnToStavro.notmetered
meMA.Ode3Kathisma1.incipit = "O Apostles, you have been…"
meMA.Ode3Kathisma1.text = "O Apostles, you have been settled in light unapproachable, as being abodes of light. And now you ever illuminate this holy house of yours, with your divine presence. We therefore cry to you with faith: Implore the Creator for us, and deliver us from the darkness, from enemy invasion, and from any kind of danger."

meMA.Ode4C1H.ode = miscellanea_en_US_unknown.misc.Ode4
meMA.Ode4C1H.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode4C1H.greekmelody = he.h.m4_en_US_unknown.heHE.TinAnexichniaston.greekname
meMA.Ode4C1H.melody = he.h.m4_en_US_unknown.heHE.TinAnexichniaston.melody
meMA.Ode4C1H.incipit = he.h.m4_en_US_unknown.heHE.TinAnexichniaston.incipit
meMA.Ode4C1H.text = he.h.m4_en_US_unknown.heHE.TinAnexichniaston.text

meMA.Ode4C21.ode = miscellanea_en_US_unknown.misc.Ode4
meMA.Ode4C21.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode4C21.greekmelody = he.h.m4_en_US_unknown.heHE.EparthentaSeIdousa.greekname
meMA.Ode4C21.melody = he.h.m4_en_US_unknown.heHE.EparthentaSeIdousa.notmetered
meMA.Ode4C21.incipit = "When the sound of the…"
meMA.Ode4C21.text = "When the sound of the Apostles' divine words went through all the world like a burning fire, it consumed the fuel of deceptive error, and with grace it illumined those who believed."

meMA.Ode4C22.ode = miscellanea_en_US_unknown.misc.Ode4
meMA.Ode4C22.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode4C22.melody = he.h.m4_en_US_unknown.heHE.EparthentaSeIdousa.notmetered
meMA.Ode4C22.incipit = "The world was in a…"
meMA.Ode4C22.text = "The world was in a dark night of ungodliness. When the disciples came, like luminaries of divine light, they illumined it with the rays of grace and the light of their preaching."

meMA.Ode4C23.ode = miscellanea_en_US_unknown.misc.Ode4
meMA.Ode4C23.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode4C23.melody = he.h.m4_en_US_unknown.heHE.EparthentaSeIdousa.notmetered
meMA.Ode4C23.incipit = "O all-blessed Apostles, you were…"
meMA.Ode4C23.text = "O all-blessed Apostles, you were made brilliant with the sacred flames of the noetic Sun. Like stars you shine on the world, and with the light of Godhead you drive away the darkness of error."

meMA.Ode4C24.ode = miscellanea_en_US_unknown.misc.Ode4
meMA.Ode4C24.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode4C24.melody = he.h.m4_en_US_unknown.heHE.EparthentaSeIdousa.notmetered
meMA.Ode4C24.incipit = "O Logos, Your eye-witnesses had…"
meMA.Ode4C24.text = "O Logos, Your eye-witnesses had Your Cross as a powerful rod, with which they parted the sea of life. Like horses, they troubled the waves of paganism."

meMA.Ode4C2T.ode = miscellanea_en_US_unknown.misc.Ode4
meMA.Ode4C2T.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode4C2T.melody = he.h.m4_en_US_unknown.heHE.EparthentaSeIdousa.notmetered
meMA.Ode4C2T.incipit = "O Christ, the immaculate Virgin…"
meMA.Ode4C2T.text = "O Christ, the immaculate Virgin is like Your heaven, O King of kings. She is glorified as being the Theotokos, and she is adorned with light as with a garment of varied colors."

meMA.Ode5C1H.ode = miscellanea_en_US_unknown.misc.Ode5
meMA.Ode5C1H.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode5C1H.greekmelody = he.h.m4_en_US_unknown.heHE.ExestiTaSympanta.greekname
meMA.Ode5C1H.melody = he.h.m4_en_US_unknown.heHE.ExestiTaSympanta.melody
meMA.Ode5C1H.incipit = he.h.m4_en_US_unknown.heHE.ExestiTaSympanta.incipit
meMA.Ode5C1H.text = he.h.m4_en_US_unknown.heHE.ExestiTaSympanta.text

meMA.Ode5C21.ode = miscellanea_en_US_unknown.misc.Ode5
meMA.Ode5C21.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode5C21.greekmelody = he.h.m4_en_US_unknown.heHE.SyKyrieMouFos.greekname
meMA.Ode5C21.melody = he.h.m4_en_US_unknown.heHE.SyKyrieMouFos.notmetered
meMA.Ode5C21.incipit = "O Christ, You made Your…"
meMA.Ode5C21.text = "O Christ, You made Your divine and all-wise disciples to be like light in the world. To all they declared You to be the unwaning Light."

meMA.Ode5C22.ode = miscellanea_en_US_unknown.misc.Ode5
meMA.Ode5C22.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode5C22.melody = he.h.m4_en_US_unknown.heHE.SyKyrieMouFos.notmetered
meMA.Ode5C22.incipit = "O Apostles, you possessed every…"
meMA.Ode5C22.text = "O Apostles, you possessed every virtue, and you crushed the demon's many snares of wickedness."

meMA.Ode5C23.ode = miscellanea_en_US_unknown.misc.Ode5
meMA.Ode5C23.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode5C23.melody = he.h.m4_en_US_unknown.heHE.SyKyrieMouFos.notmetered
meMA.Ode5C23.incipit = "The holy Apostles spoke with…"
meMA.Ode5C23.text = "The holy Apostles spoke with fiery tongues. They gave us a glimpse of the light of the Godhead, the Unity in Trinity."

meMA.Ode5C2T.ode = miscellanea_en_US_unknown.misc.Ode5
meMA.Ode5C2T.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode5C2T.melody = he.h.m4_en_US_unknown.heHE.SyKyrieMouFos.notmetered
meMA.Ode5C2T.incipit = "O Bride of God, we…"
meMA.Ode5C2T.text = "O Bride of God, we have you as the anchor and hope of our salvation, and we present you as an indestructible shield against our enemies."

meMA.Ode6C1H.ode = miscellanea_en_US_unknown.misc.Ode6
meMA.Ode6C1H.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode6C1H.greekmelody = he.h.m4_en_US_unknown.heHE.TinTheianTaftin.greekname
meMA.Ode6C1H.melody = he.h.m4_en_US_unknown.heHE.TinTheianTaftin.melody
meMA.Ode6C1H.incipit = he.h.m4_en_US_unknown.heHE.TinTheianTaftin.incipit
meMA.Ode6C1H.text = he.h.m4_en_US_unknown.heHE.TinTheianTaftin.text

meMA.Ode6C21.ode = miscellanea_en_US_unknown.misc.Ode6
meMA.Ode6C21.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode6C21.greekmelody = he.h.m4_en_US_unknown.heHE.ThysoSoiMetaFonis.greekname
meMA.Ode6C21.melody = he.h.m4_en_US_unknown.heHE.ThysoSoiMetaFonis.notmetered
meMA.Ode6C21.incipit = "O Savior, You established Your…"
meMA.Ode6C21.text = "O Savior, You established Your disciples in grace and wisdom, You made them stronger than the sophistries of the Hellenes, whose erroneous teachings You confounded."

meMA.Ode6C22.ode = miscellanea_en_US_unknown.misc.Ode6
meMA.Ode6C22.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode6C22.melody = he.h.m4_en_US_unknown.heHE.ThysoSoiMetaFonis.notmetered
meMA.Ode6C22.incipit = "The Apostles were like divine…"
meMA.Ode6C22.text = "The Apostles were like divine rivers of wisdom, running with streams that flow from the wellsprings of salvation. With their saving waters they filled the Church."

meMA.Ode6C23.ode = miscellanea_en_US_unknown.misc.Ode6
meMA.Ode6C23.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode6C23.melody = he.h.m4_en_US_unknown.heHE.ThysoSoiMetaFonis.notmetered
meMA.Ode6C23.incipit = "O all-blessed Apostles, you were…"
meMA.Ode6C23.text = "O all-blessed Apostles, you were like stars, shining with the splendor of knowledge of God. With glittering rays of light, you dispelled the darkness of error."

meMA.Ode6C2T.ode = miscellanea_en_US_unknown.misc.Ode6
meMA.Ode6C2T.mode = miscellanea_en_US_unknown.misc.Mode4
meMA.Ode6C2T.melody = he.h.m4_en_US_unknown.heHE.ThysoSoiMetaFonis.notmetered
meMA.Ode6C2T.incipit = "O Mother of God, the…"
meMA.Ode6C2T.text = "O Mother of God, the spiritual Bridegroom found you, like that fair and blameless companion, like the flower of the valleys, and He dwelt in you."

meMA.Oikos1.text = ""

meMA.Ode7C1H.ode = miscellanea_en_US_unknown.misc.Ode7
meMA.Ode7C1H.mode = miscellanea_en_US_unknown.misc.Mode4
